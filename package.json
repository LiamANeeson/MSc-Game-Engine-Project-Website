{
  "name": "game_engine_website",
  "version": "1.0.0",
  "description": "Website for MSc project game engine ",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "stop": "taskkill -f -im node.exe",
    "data:import": "node dbSeeder",
    "data:destroy": "node dbSeeder -d",
    "test": "mocha",
    "test-with-coverage": "nyc --reporter=text mocha"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiamANeeson/MSc-Game-Engine-Project-Website.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LiamANeeson/MSc-Game-Engine-Project-Website/issues"
  },
  "homepage": "https://github.com/LiamANeeson/MSc-Game-Engine-Project-Website#readme",
  "dependencies": {
    "@types/chai": "^4.3.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "concurrently": "^7.4.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb-memory-server": "^8.9.3",
    "mongoose": "^6.6.4",
    "nodemon": "^2.0.20",
    "should": "^13.2.3",
    "nyc": "^15.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "react-select": "^5.5.2",
    "sinon": "^14.0.1",
    "supertest": "^6.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20"
  }
}
